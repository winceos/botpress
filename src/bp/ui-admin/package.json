{
  "name": "admin-ui",
  "version": "0.0.1",
  "private": true,
  "publisher": "botpress",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "cross-env BROWSER=none react-app-rewired start",
    "start": "cross-env PUBLIC_URL=admin npm-run-all -p watch-css start-js",
    "start:dev": "cross-env PUBLIC_URL=admin cross-env REACT_APP_API_URL=http://localhost:3000 cross-env PORT=3001 npm-run-all -p watch-css start-js",
    "build-js": "react-app-rewired build",
    "build": "yarn build-css && cross-env PUBLIC_URL=admin yarn build-js",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "clear-cache": "rimraf node_modules/.cache"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.23.1",
    "@blueprintjs/select": "^3.11.2",
    "@botpress/util-roles": "^10.36.1",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.5",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "element-closest-polyfill": "^1.0.0",
    "firebase": "^5.9.1",
    "jdenticon": "^2.1.1",
    "joi-browser": "^13.4.0",
    "moment": "^2.24.0",
    "ms": "^2.1.1",
    "nanoid": "^2.0.3",
    "query-string": "^5.0.1",
    "react": "16.8.6",
    "react-checkbox-tree": "^1.5.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "16.8.6",
    "react-icons": "^3.8.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^2.1.8",
    "react-select": "^2.4.2",
    "react-table": "^6.11.5",
    "reactstrap": "^8.4.0",
    "recharts": "^1.5.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "snarkdown": "^1.2.2"
  },
  "devDependencies": {
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.0",
    "@types/react-router-redux": "^5.0.18",
    "@types/react-select": "^2.0.19",
    "@types/reactstrap": "^8.4.1",
    "cross-env": "^5.2.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "^2.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
